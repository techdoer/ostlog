{
	"definitions": {
		"domain": {
			"type": "string",
			"enum": ["physical", "emotional", "spiritual", "intellectual", "environmental", "social", "financial"],
			"description": "one of the seven dimensions of well-being"
		},
		"likert_scale": {
			"type": "string",
			"enum": ["strongly disagree", "disagree", "neutral", "agree", "strongly agree"],
			"description": "a standard likert scale"
		},
		"time_units": {
			"type": "string",
			"enum": ["s", "min", "h", "d", "wk", "mo", "a"],
			"description": "unit of measure for the duration."
		},
		"status": {
			"type": "string",
			"enum": ["active", "completed", "entered-in-error", "intended", "stopped", "on-hold", "unknown", "not-taken"],
			"description": "the status of administering the drug.  Value set taken from http://hl7.org/fhir/ValueSet/medication-statement-status."
		},

	}
