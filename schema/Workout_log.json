{

	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/Surgery_log.json",
	"title": "Surgery log",
	"description": "a surgical intervention",
	"type": "object",
	"properties": {

		"id": {
			"type": "integer"
		},
		"domain ": {
			"$ref": "definitions.json#/domain"
		},
		"calendar_date_id": {
			"type": "string",
			"description": "YYYYMMDD formatted date of the measurement(s)"
		},
		"timestamp": {
			"type ": "string ",
			"description": "HH:MM:SS formatted time of the measurement(s)"
		},
		"person_id ": {
			"type": "integer",
			"description ": "the person to whom the metrics phenotype and physiological metrics apply."
		},
		"workout_type": {
			"type": "string",
			"description": ""
		},
		"duration_minutes": {
			"type": "float"
		},
		"distance_miles": {
			"type": "float"
		},
		"elevation_gain_ft": {
			"type": "float"
		},
		"avg_speed_mph": {
			"type": "float"
		},
		"max_speed_mph": {
			"type": "float"
		},
		"calories_burned": {
			"type": "float"
		},
		"remarks": {
			"type": "string"
		},
		"required": ["id", "domain", "calendar_date_id", "timestamp", "person_id"]

	}
}
