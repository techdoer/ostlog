{

	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/Workout_log.json",
	"title": "Workout log",
	"description": "Each entry describes a workout in time.",
	"type": "object",
	"properties": {

		"id": {
			"type": "integer"
		},
		"calendar_date_id": {
			"type": "string",
			"description": "YYYYMMDD formatted date of the workout."
		},
		"timestamp": {
			"type ": "string ",
			"description": "HH:MM:SS formatted time of the workout."
		},
		"domain ": {
			"$ref": "definitions.json#/domain"
		},
		"person_id ": {
			"type": "integer",
			"description ": "the person who worked out."
		},
		"workout_type": {
			"type": "string",
			"enum": ["walking", "running", "swimming", "cycling", "hiking", "other"],
			"description": ""
		},
		"duration_minutes": {
			"type": "float",
			"description": ""
		},
		"distance_miles": {
			"type": "float",
			"description": ""
		},
		"elevation_gain_ft": {
			"type": "float",
			"description": ""
		},
		"avg_speed_mph": {
			"type": "float",
			"description": ""
		},
		"max_speed_mph": {
			"type": "float",
			"description": ""
		},
		"calories_burned": {
			"type": "float",
			"description": ""
		},
		"remarks": {
			"type": "string"
		},
		"required": ["id", "calendar_date_id", "timestamp", "domain", "person_id"]

	}
}
