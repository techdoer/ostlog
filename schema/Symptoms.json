{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/symptoms.json",
	"title": "People",
	"description": "A health symptom.",
	"type": "object",
	"properties": {
		"id": {
			"type": "integer"
		},
		"sname": {
			"type": "string",
			"description": "textual name of the symptom"
		},
		"classification": {
			"type": "string",
			"description": "unique classification for the symptom"
		},
		"remarks": {
			"type": "string"
		}
	},
	"required": ["id", "sname", "classification"]
}
