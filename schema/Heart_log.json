{

	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/schema/Heart_log.json",
	"title": "Heart log",
	"description": "Each entry characterises results from heart examinations or ECG.",
	"type": "object",
	"properties": {

		"id": {
			"type": "integer"
		},
		"calendar_date_id": {
			"type": "string",
			"description": "YYYYMMDD formatted date when heart observation made."
		},
		"timestamp": {
			"type ": "string ",
			"description": "HH:MM:SS formatted time when heart observation made."
		},
		"domain ": {
			"$ref": "definitions.json#/domain"
		},
		"person_id ": {
			"type": "integer",
			"description ": "the person to whose heart is being tested."
		},
		"min_heart_rate_bpm": {
			"type": "float"
		},
		"avg_heart_rate_bpm": {
			"type": "float"
		},
		"max_heart_rate_bpm": {
			"type": "float"
		},
		"hrv": {
			"type": "float"
		},
		"hrv_rmssd": {
			"type": "float"
		},
		"hrv_sdnn": {
			"type": "float"
		},
		"ecg_pr_ms": {
			"type": "integer",
			"description": "PR interval milliseconds"
		},
		"ecg_p_ms": {
			"type": "integer",
			"description": "p wave"
		},
		"ecg_qrs_ms": {
			"type": "integer",
			"description": "QRS complex milliseconds"
		},
		"ecg_t_ms": {
			"type": "integer",
			"description": "t wave"
		},
		"ecg_qt_ms": {
			"type": "integer",
			"description": "QT interval milliseconds"
		},
		"ecg_qtc_ms": {
			"type": "integer",
			"description": "QTC interval milliseconds"
		},
		"ecg_p_axis": {
			"type": "integer"
		},
		"ecg_qrs_axis": {
			"type": "integer"
		},
		"ecg_t_axis": {
			"type": "integer"
		},
		"ecg_rv1_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"ecg_sv5_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"ecg_rv5_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"ecg_sv1_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"remarks": {
			"type": "string"
		},

		"required": ["id", "calendar_date_id", "timestamp", "domain", "person_id"]
	}
}
