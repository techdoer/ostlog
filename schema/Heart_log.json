{

	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/Heart_log.json",
	"title": "Heart log",
	"description": "Heart and ECG results.",
	"type": "object",
	"properties": {

		"id": {
			"type": "integer"
		},
		"calendar_date_id": {
			"type": "string",
			"description": "YYYYMMDD formatted date of the measurement(s)"
		},
		"timestamp": {
			"type ": "string ",
			"description": "HH:MM:SS formatted time of the measurement(s)"
		},
		"person_id ": {
			"type": "integer",
			"description ": "the person to whom the metrics phenotype and physiological metrics apply."
		},
		"domain ": {
			"$ref": "definitions.json#/domain"
		},
		"min_heart_rate_bpm": {
			"type": "float"
		},
		"avg_heart_rate_bpm": {
			"type": "float"
		},
		"max_heart_rate_bpm": {
			"type": "float"
		},
		"hrv": {
			"type": "float"
		},
		"hrv_rmssd": {
			"type": "float"
		},
		"hrv_sdnn": {
			"type": "float"
		},
		"ecg_pr_ms": {
			"type": "integer",
			"description": "PR interval milliseconds"
		},
		"ecg_p_ms": {
			"type": "integer",
			"description": "p wave"
		},
		"ecg_qrs_ms": {
			"type": "integer",
			"description": "QRS complex milliseconds"
		},
		"ecg_t_ms": {
			"type": "integer",
			"description": "t wave"
		},
		"ecg_qt_ms": {
			"type": "integer",
			"description": "QT interval milliseconds"
		},
		"ecg_qtc_ms": {
			"type": "integer",
			"description": "QTC interval milliseconds"
		},
		"ecg_p_axis": {
			"type": "integer"
		},
		"ecg_qrs_axis": {
			"type": "integer"
		},
		"ecg_t_axis": {
			"type": "integer"
		},
		"ecg_rv1_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"ecg_sv5_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"ecg_rv5_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"ecg_sv1_mv": {
			"type": "float",
			"description": " amplification millivolts"
		},
		"remarks": {
			"type": "string"
		},

		"required": ["id", "calendar_date_id", "timestamp", "person_id", "domain"]
	}
}
