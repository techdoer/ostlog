{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/schema/Holidays_log.json",
	"title": "Holidays log",
	"description": "Dates of holidays with related metadata.",
	"type": "object",
	"properties": {
		"id": {
			"type": "integer"
		},
		"meta_id": {
			"type": "integer",
			"description": "reference to entry's metadata"
		},
		"calendar_date_id": {
			"type": "integer",
			"description": "YYYYMMDD unique identifier"
		},
		"name": {
			"type": "string",
			"description": "name of holiday"
		},
		"remarks": {
			"type": "string"
		}
    }
}