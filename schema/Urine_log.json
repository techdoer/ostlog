{

	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/Urine_log.json",
	"title": "Urine log",
	"description": "Each entry records results from a urine lab test.",
	"type": "object",
	"properties": {

		"id": {
			"type": "integer"
		},
		"calendar_date_id": {
			"type": "string",
			"description": "YYYYMMDD formatted date of the urine sample."
		},
		"timestamp": {
			"type ": "string ",
			"description": "HH:MM:SS formatted time of the urine sample."
		},
		"domain ": {
			"$ref": "definitions.json#/domain"
		},
		"person_id ": {
			"type": "integer",
			"description ": "the person to whom the urine belongs."
		},
		"clarity": {},
		"color": {},
		"density_15_c": {},
		"ph": {},
		"albumin": {},
		"glucose": {},
		"ketones": {},
		"urobilinogen": {},
		"bilirubin": {},
		"hemoglobin": {},
		"nitries": {},
		"remarks": {
			"type": "string"
		},
		"required": ["id", "calendar_date_id", "timestamp", "domain", "person_id"]

	}
}
