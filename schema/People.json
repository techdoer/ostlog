{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/People.json",
	"title": "People",
	"description": "A person referenced in ostlog.",
	"type": "object",
	"properties": {
		"id": {
			"type": "integer"
		},
		"first_name": {
			"type": "string"
		},
		"last_name": {
			"type": "string"
		},
		"date_of_birth": {
			"type": "integer",
			"description": "formatted YYYYMMDD and references 'Calender dates'"
		},
		"remarks": {
			"type": "string"
		}
	},
	"required": ["id", "first_name", "last_name"]
}
