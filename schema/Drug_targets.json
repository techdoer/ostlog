{

	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://ostlog.org/schema/Drug_targets.json",
	"title": "Drug targets",
	"description": "Each entry represets a drug and the disorder it targets.",
	"type": "object",
	"properties": {
		"drug_atc_code": {
			"type": "string"
		},
		"disorder_sctid": {
			"type": "integer",
			"description": "YYYYMMDD formatted date when symptom experienced."
		},
		"disorder_name": {
			"type": "string"
		},
		"remarks": {
			"type": "string"
		}
	},
	"required": ["drug_atc_code", "disorder_sctid", "disorder_name"]
}
